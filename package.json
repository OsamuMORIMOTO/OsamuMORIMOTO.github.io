{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.5.0"
  },
  "scripts": {
    "hexo": "hexo",
    "watch": "hexo generate -w --draft",
    "watch-post": "hexo generate -w",
    "generate": "hexo generate --draft",
    "generate-post": "hexo generate",
    "server": "hexo server -s -i 192.168.33.10",
    "new": "hexo new",
    "draft": "hexo new draft",
    "post": "hexo new post",
    "publish": "hexo publish post",
    "clean": "hexo clean",
    "deploy": "hexo deploy -g",
    "clean-deploy": "run-s clean deploy",
    "c-d": "run-s clean deploy",
    "start": "run-p watch server"
  },
  "dependencies": {
    "hexo": "^3.2.0",
    "hexo-deployer-git": "^0.3.1",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-inject": "^1.0.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-jade": "^0.4.1",
    "hexo-renderer-markdown-it-plus": "^1.0.2",
    "hexo-renderer-sass": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-tag-katex": "^1.1.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  }
}
